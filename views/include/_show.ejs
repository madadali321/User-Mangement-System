<% for( let i = 0; i < users.length; i++ ) { %>
    <tr>
        <td><%= i+1 %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].email %></td>
        <td><%= users[i].gender %></td>
        <td><%= users[i].status %></td>
        <td>
            <a href="/update-user" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete">
                <span class="text-gradient"><i class="fa-solid fa-circle-xmark"></i></span>
            </a>
            <a href="/add-order" class="btn border-shadow update">
                <span class="text-gradient"></span><i class="fa-solid fa-cart-plus" style="color: rebeccapurple;"></i>
            </a>
        </td>
    </tr>
<% } %>
